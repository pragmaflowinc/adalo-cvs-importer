    {
      "name": "uploadTrue",
      "displayName": "To start the upload, set this to true",
      "type": "text"
    },
    {
      "name": "worksheetHaveHeaders",
      "displayName": "Does the worksheet have a header row (i.e. skip the first row if this is 'true')? ",
      "type": "text"
    },
    {
      "name": "textToDiplay",
      "displayName": "Text to display",
      "type": "text",
      "defaultValue": "Drag and drop here, or",
      "styles": {
        "fontFamily": "@body",
        "color": "@text",
        "fontWeight": "normal"
      }
    },
    {
      "name": "textToDiplayOnDrop",
      "displayName": "Text to display when a file is about to be dropped",
      "type": "text",
      "defaultValue": "Drop here ;)",
      "styles": {
        "fontFamily": "@body",
        "color": "@text",
        "fontWeight": "normal"
      }
    },
    {
      "name": "browseTextToDiplay",
